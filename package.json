  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": ""
      }
    ],
    "asar": true,
    "asarUnpack": [
      "public"
    ],
    "appId": "com.xeajs",
    "productName": "xeajs Electron",
    "files": [
      "dist",
      "public",
      "index.js",
      "config.js",
      "!node_modules",
      "!**/*.{lib,obj,pdb,cc,h,tlog,map,exp,gypi,vcxproj,filters,cpp,c,ts}"
    ],
    "nodeGypRebuild": false,
    "extraFiles": [],
    "win": {
      "icon": "public/assets/favicon/png/favicon@5x.png",
      "requestedExecutionLevel": "asInvoker"
    },
    "mac": {
      "icon": "public/assets/favicon/icons/favicon@5x.icns"
    },
    "linux": {
      "icon": "public/assets/favicon/icons/favicon@5x.icns",
      "category": "AudioVideo",
      "target": "deb",
      "executableName": "xeajs Electron"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "language": 2052
    },
    "directories": {
      "output": "output"
    }
  },
  "window": {
    "center": true,
    "title": "Electron",
    "show": false,
    "width": 960,
    "height": 640,
    "frame": false,
    "webPreferences": {
      "webSecurity": true,
      "enableRemoteModule": true,
      "nodeIntegration": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": "eslint --color --fix --ext .ts,.tsx"
  }
}